{
  "name": "marijnvanwezel/type-enum",
  "description": "An enum of all native PHP types.",
  "type": "library",
  "keywords": [
    "enum",
    "type"
  ],
  "homepage": "https://github.com/marijnvanwezel/type-enum",
  "readme": "https://github.com/marijnvanwezel/type-enum/README.md",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Marijn van Wezel",
      "email": "marijnvanwezel@gmail.com",
      "homepage": "https://www.marijn.it",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/marijnvanwezel/type-enum/issues",
    "source": "https://github.com/marijnvanwezel/type-enum"
  },
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "MarijnVanWezel\\TypeEnum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MarijnVanWezel\\TypeEnum\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "8.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "infection/infection": "^0.26.4",
    "phpunit/phpunit": "^9.5"
  }
}
